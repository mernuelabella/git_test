/* #Custom Styles
================================================== */
.disInfo img {
    padding: 0 10%;
  	width: 15%;
}
.disInfo {
    text-align: center;
}
.linkDes {
    height: 160px;
    margin-bottom: 20px;
    position: relative;
}
.linkDes span {
    position: absolute;
    bottom: 0;
}
.awardDes div {
    height: 275px;
    margin: 20px 0;
}
/*.flexslider .slides {
  background: url('gray-light.png')
}*/
.nav ul.mobile_menu {
    background-color: #BBD406;
}
.nav ul.mobile_menu li {
    border-top: 1px solid #BBD406;
}
.nav ul.mobile_menu li a {
    color: #ffffff;
}
.nav ul.mobile_menu li.active {
    background-color: #829309;
}
.mobile_menu li:hover {
    background-color: #A3B711;
}
.toggle span {
    color: #ffffff;
}
.nav ul.mobile_menu li:first-child {
    padding: 1px;
}
.relative.product_image img {
    width: initial;
    height: 205px;
}
.four.columns.thumbnail {
    height: 320px;
    margin-top: 20px;
}
.thumbnail .price {
    position: absolute;
    /* left: 50%; */
    /* right: 50%; */
    left: 50%;
    margin-right: -50%;
    transform: translate(-50%, -50%);
    bottom: 0;
}
.thumbnail .title {
    line-height: 20px;
}
.product .flexslider .slides {
    background: white;
}
.clickCom {
    margin-top: 10px;
    margin-bottom: 30px;
    font-size: 20px;
    text-align: center;
    text-transform: uppercase;
}
.clickCom a:hover {
    color: #bbd406;
}
#map {
    display: block;
    /*width: 600px !important;*/
    height: 600px !important;
}
.mapWrapper {
    width: 90%;
    margin: 0 auto;
}
input#map-search-box {
    width: 100%;
    text-align: center;
}
ul.store-list {
    list-style: none;
    margin: 0;
    border-radius: 5px;
    border: 1px solid #d7dcde;
    height: 600px !important;
    overflow: auto;
}
ul.store-list li {
    padding: 10px;
    border-bottom: 1px solid #d7dcde;
    margin: 0;
    line-height: 25px;
    font-size: 12px;
}
ul.store-list li:hover {
    background-color: rgba(215, 220, 222, 0.26);
}
.list-store-name {
    font-size: 15px;
    font-weight: bold;
    text-transform: uppercase;
    padding-bottom: 5px;
}
.store-list-container {
    width: 30%;
    margin-left: 0;
    float: left;
    display: inline;
    margin-right: 10px;
}
.list-store-phone, .list-store-address, .list-store-website {
    color: #a9a9a9;
    text-transform: uppercase;
    font-weight: bold;
}
p.map-store-name {
    font-size: 16px;
    font-weight: bold;
    margin: 0;
}
p.map-store-address {
    margin: 0;
    color: #a9a9a9;
}
.ermsgStore {
    color: #a9a9a9;
    padding: 30px;
    font-size: 15px;
    text-align: center;
    text-transform: uppercase;
    font-weight: bold;
}
#homepage_slider .slides img, .article .slides img {
    width: 100%;
}
div#instafeed img {
    width: 80%;
}
div#instafeed {
    text-align: center;
}
.descrDes img, .descrDes iframe {
    margin: 0 auto;
    margin-bottom: 20px;
    display: block;
}
.descrDes h3 {
    margin: 0 0 15px 0;
    text-align: left;
    padding: 0;
    border-bottom: 1px solid #dbe3e5;
    padding-bottom: 8px;
}
.swatch label img {
    float: left;
    padding: 5px;
}
.swatch label {
    height: initial;
    padding: 0;
}
.swatch-element.color {
    padding: 0;
    border: 0;
}
.color.swatch-element label {
    height: initial;
}
.flex-control-nav li {
    display: none;
}
/*.flex-control-nav li:first-child {
    display: block;
    margin: 0 auto;
}*/
body.product h5 {
    padding: 10px 0;
}